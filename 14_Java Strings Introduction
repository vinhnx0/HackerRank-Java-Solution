import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        String B=sc.next();
        /* Enter your code here. Print output to STDOUT. */
        // Get length
        int len = A.length() + B.length();
        // Compare alphabetical order (A>B)?
        String alphabet = A.compareTo(B)>0? "Yes":"No";
        // Concatenate A and B
        String name = A.substring(0,1).toUpperCase() + A.substring(1) + " " + B.substring(0,1).toUpperCase() + B.substring(1);
        // Print the result
        System.out.println(len);
        System.out.println(alphabet);
        System.out.println(name);

    }
}
